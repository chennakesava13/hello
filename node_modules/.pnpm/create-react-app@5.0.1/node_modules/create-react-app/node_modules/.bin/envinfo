#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules/envinfo/dist/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules/envinfo/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/create-react-app@5.0.1/node_modules/create-react-app/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/create-react-app@5.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules/envinfo/dist/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules/envinfo/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/envinfo@7.13.0/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/create-react-app@5.0.1/node_modules/create-react-app/node_modules:/home/workspace/reactjs/coding-practices/superOverLeagueApp/node_modules/.pnpm/create-react-app@5.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../envinfo@7.13.0/node_modules/envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../envinfo@7.13.0/node_modules/envinfo/dist/cli.js" "$@"
fi
